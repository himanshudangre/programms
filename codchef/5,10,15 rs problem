#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    int A[n];
	    int i;
	    for(i=0;i<n;i++)
	    {
	        cin>>A[i];
	    }
	   int j=0;
	   int k=0;
	 
	    while( A[0]==5 && A[j+1]<=A[j]+5)
	    {
	        k++;
	        j++;
	    }
	    if(k==n)
	        cout<<"YES"<<endl;
	    else
	        cout<<"NO"<<endl;
	        

	}
	return 0;
}
